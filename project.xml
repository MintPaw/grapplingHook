<?xml version="1.0" encoding="utf-8"?>
<project>
	
	<meta title="GrapplingHook" package="com.sample.grapplinghook" version="1.0.0" company="Company Name" />
	<app main="Main" path="bin" file="GrapplingHook" />
	<window width="1280" height="720" />
	<set name="SWF_VERSION" value="12" />

	<set name="telemetry" if="debug" />
	<haxedef name="HXCPP_DEBUGGER" if="debug" />
	<haxedef name="HXCPP_STACK_TRACE" if="debug" />
	<haxedef name="HXCPP_TELEMETRY" if="debug" />
	<haxelib name="debugger" if="debug" />
	<haxelib name="hxtelemetry" if="debug" />
	
	<source path="src" />
	
	<haxelib name="openfl" />
	<haxelib name="flixel" />
	<haxelib name="flixel-addons" />
	
	<assets path="assets" />
	
</project>
